{
  "dependencies": {
    "bootstrap": "^5.1.3",
    "esbuild": "^0.14.39",
    "sass": "^1.52.1"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds",
    "build:css": "sass ./app/assets/stylesheets/app_css.sass.scss ./app/assets/builds/app_css.css --no-source-map --load-path=node_modules"
  }
}
